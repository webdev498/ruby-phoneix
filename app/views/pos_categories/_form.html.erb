<%= simple_form_for(@pos_category) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :dept_number %>
    <%= f.input :category_abbreviation %>
    <%= f.input :category_description %>
    <%= f.input :taxable %>
    <%= f.input :medical %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
