                          <tr>
                            <td> <%= i = i +1 %> </td>
                            <td> <%= f.plan_id_code %> </td>
                            <td> <%= f.plan_abb_name %> </td>
                            <td> <%= f.bin_number %> </td>
                            <td> <%= f.processor_control_number %> </td>
                            <td> <%= f.plan_type %> </td>
                            <td> <%= f.active %> </td>
                            <td style="display:none"> <%= f.id %> </td>
                          </tr>
                      <% end %>
                      </tbody>
                    </table>
                  </div>
                </row>
              </div>
