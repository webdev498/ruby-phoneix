                  </div>
                  <div class="container-fluid">
                    <table class="table table-hover phx-table phx-search-font" id="customer-plan-tbl">
                      <thead class="phx-table-head">
                        <tr>
                          <th style="background-color:#f9f9fa;" ></th>
                          <th style="background-color:#f9f9fa;" >Plan ID Code</th>
                          <th style="background-color:#f9f9fa;" >Plan Name </th>
                          <th style="background-color:#f9f9fa;" >Bin #</th>
                          <th style="background-color:#f9f9fa;" >Processor Ctrl#</th>
                          <th style="background-color:#f9f9fa;" >Plan Type</th>
                          <th style="background-color:#f9f9fa;" >Active</th>
                        </tr>
                      </thead>
                      <tbody>
                          <% i = 0 %>
                          <% @customer.customerPlans.joins("LEFT JOIN plans ON plans.plan_id_code = customer_plans.plan_id_code")
